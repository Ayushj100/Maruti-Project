    function preloadImg(deskQuery, mobQuery) {
      const isDesktop = window.matchMedia('(min-width: 600px)').matches;
      const src = document.querySelector(`${isDesktop ? deskQuery : mobQuery}`)?.href;
      if (!src) {
        return;
      }
      const breakpoints = { desktop: 2000, mobile: 750 };
      const dmUrl = new URL(src, window.location.href);
      const currentExtension = dmUrl.pathname.substring(dmUrl.pathname.lastIndexOf('.') + 1).toLowerCase();
      if (!['svg', 'gif'].includes(currentExtension)) {
        if (!['jpg', 'jpeg'].includes(currentExtension)) {
          dmUrl.href = `${dmUrl.href.substring(0, dmUrl.href.lastIndexOf('.'))}.png`;
        }
        dmUrl.searchParams.set('width', isDesktop ? breakpoints.desktop : breakpoints.mobile);
        dmUrl.searchParams.set('id', '1');
        dmUrl.searchParams.set('preferwebp', 'true');
      }
      const img = new Image();
      img.loading = 'eager';
      img.fetchPriority = 'high';
      img.src = dmUrl.href;

      return img;
    }
